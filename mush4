#include <iostream>
using namespace std;

int main() {
    int a[3];
    for (int i = 0; i < 3; i++) {
        cin >> a[i];
    }
    int max1 = a[0], max2 = a[1], max2_i = 1;
    if (max2 > max1) {
        swap(max1, max2);
        max2_i = 0;
    }
    if (a[2] > max1) {
        max2 = max1;
        max2_i = max1 == a[0] ? 1 : 0;
        max1 = a[2];
    } else if (a[2] > max2) {
        max2 = a[2];
        max2_i = max1 == a[0] ? 1 : 0;
    }
    cout << "max2_3x = " << max2 << endl;
    cout << "max2_3i = " << max2_i << endl;
    return 0;
}
